<sdf version='1.6'>
  <model name='quadcopter'>
    <link name='main_body'>
      <pose frame=''>0 0 0 0 0 0</pose>
      <inertial>
        <pose frame=''>0 0 0 0 0 0</pose>
        <mass>2.24</mass>
        <inertia>
          <ixx>0.0363</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0363</iyy>
          <iyz>0</iyz>
          <izz>0.0615</izz>
        </inertia>
      </inertial>
      <collision name='main_body_collision'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/main_body.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name='main_body_visual'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/main_body_urss.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <link name='motor3'>
      <pose frame=''>-0.332 0 0 0 0 0</pose>
      <inertial>
        <pose frame=''>0 0 0 0 0 0</pose>
        <mass>0.00007333</mass>
        <inertia>
          <ixx>1e-10</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-10</iyy>
          <iyz>0</iyz>
          <izz>1e-10</izz>
        </inertia>
      </inertial>
      <collision name='motor3_collision'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/motor1.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name='motor3_visual'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/motor1urss.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <joint name='prp3' type='revolute'>
      <child>motor1</child>
      <parent>main_body</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>20</effort>
          <velocity>10000</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
    <link name='motor4'>
      <pose frame=''>0 -0.332 0.0 0 0 0</pose>
      <inertial>
        <pose frame=''>0 0 0 0 0 0</pose>
        <mass>0.00007333</mass>
        <inertia>
          <ixx>1e-10</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-10</iyy>
          <iyz>0</iyz>
          <izz>1e-10</izz>
        </inertia>
      </inertial>
      <collision name='motor4_collision'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/motor2.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name='motor4_visual'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/motor2urss.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <joint name='prp4' type='revolute'>
      <child>motor2</child>
      <parent>main_body</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>20</effort>
          <velocity>10000</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
    <link name='motor1'>
      <pose frame=''>0.332 0 0 0 0 0</pose>
      <inertial>
        <pose frame=''>0 0 0 0 0 0</pose>
        <mass>0.00007333</mass>
        <inertia>
          <ixx>1e-10</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-10</iyy>
          <iyz>0</iyz>
          <izz>1e-10</izz>
        </inertia>
      </inertial>
      <collision name='motor1_collision'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/motor3.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name='motor1_visual'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/motor3urss.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <joint name='prp1' type='revolute'>
      <child>motor3</child>
      <parent>main_body</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>20</effort>
          <velocity>10000</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
    <link name='motor2'>
      <pose frame=''>0 0.332 0.0 0 0 0</pose>
      <inertial>
        <pose frame=''>0 0 0 0 0 0</pose>
        <mass>0.00007333</mass>
        <inertia>
          <ixx>1e-10</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-10</iyy>
          <iyz>0</iyz>
          <izz>1e-10</izz>
        </inertia>
      </inertial>
      <collision name='motor2_collision'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/motor4.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name='motor2_visual'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>2.77 2.77 2.77</scale>
            <uri>model://quadcopter/meshes/motor4urss.dae</uri>
