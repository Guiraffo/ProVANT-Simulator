<robot name="provant_alpha_quadrotor">
  <!-- Use with <material>Gazebo/Black</material>-->
  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0" />
  </material>

  <material name="blue">
    <color rgba="0.0 0.0 0.8 1.0" />
  </material>

  <material name="green">
    <color rgba="0.0 0.8 0.0 1.0" />
  </material>

  <material name="grey">
    <color rgba="0.2 0.2 0.2 1.0" />
  </material>

  <!-- Use with <material>Gazebo/Orange</material>-->
  <material name="orange">
    <color rgba="1 0.42353 0.03922 1.0" />
  </material>

  <material name="brown">
    <color rgba="1.0 0.81177 0.76471 1.0" />
  </material>

  <!-- Use with <material>Gazebo/Red</material>-->
  <material name="red">
    <color rgba="0.8 0.0 0.0 1.0" />
  </material>

  <material name="white">
    <color rgba="1.0 1.0 1.0 1.0" />
  </material>

  <!-- Necessary for Rviz visualization of the model -->
  <link name="base_link">
    <inertial>
      <origin xyz="-6.66133814775094E-16 -1.55431223447522E-15 0.180998857571939" rpy="0 0 0" />
      <mass value="2.2" />
      <inertia ixx="0.0117197858769209" ixy="-7.63245755057274E-17" ixz="1.16636444461938E-18" iyy="0.0117197858769209" iyz="5.58752635680239E-18" izz="0.0234029287921774" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/base_link.STL" />
      </geometry>
      <material name="">
        <color rgba="1 1 1 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/base_link.STL" />
      </geometry>
    </collision>
  </link>
  <link name="legs">
    <inertial>
      <origin xyz="5.55111512312578E-16 1.11022302462516E-15 0.0621262276194157" rpy="0 0 0" />
      <mass value="0.14623345246629" />
      <inertia ixx="0.000237434770940027" ixy="2.68509444279613E-19" ixz="-1.08873598996886E-18" iyy="0.000237434770940027" iyz="6.77626357804862E-21" izz="5.41386422243988E-05" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/legs.STL" />
      </geometry>
      <material name="">
        <color rgba="1 1 1 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/legs.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="joint1" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="legs" />
    <axis xyz="0 0 0" />
  </joint>
  <link name="prop1">
    <inertial>
      <origin xyz="0 -1.66533453693773E-16 0.0220243882624624" rpy="0 0 0" />
      <mass value="0.0900308275627489" />
      <inertia ixx="4.47942174809247E-05" ixy="2.06649562901672E-05" ixz="5.03946016627054E-20" iyy="2.521089655902E-05" iyz="-7.11700751667277E-20" izz="5.78162740600789E-05" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/prop1.STL" />
      </geometry>
      <material name="">
        <color rgba="0.752941176470588 0.752941176470588 0.752941176470588 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/prop1.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="f1" type="continuous">
    <origin xyz="0.3 0 0.2005" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="prop1" />
    <axis xyz="0 0 -1" />
    <limit effort="0" velocity="0" />
  </joint>
  <link name="prop2">
    <inertial>
      <origin xyz="2.22044604925031E-16 0 0.0220243882624623" rpy="0 0 0" />
      <mass value="0.090030827562749" />
      <inertia ixx="4.4794217480925E-05" ixy="2.06649562901671E-05" ixz="-6.3683033054006E-21" iyy="2.52108965590198E-05" iyz="-3.19398545463806E-21" izz="5.7816274060079E-05" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/prop2.STL" />
      </geometry>
      <material name="">
        <color rgba="0.752941176470588 0.752941176470588 0.752941176470588 1" />
