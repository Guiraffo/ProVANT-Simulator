    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/prop2.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="f2" type="continuous">
    <origin xyz="0 0.3 0.2005" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="prop2" />
    <axis xyz="0 0 -1" />
    <limit effort="0" velocity="0" />
  </joint>
  <link name="prop3">
    <inertial>
      <origin xyz="-1.11022302462516E-16 0 0.0220243882624621" rpy="0 0 0" />
      <mass value="0.0900308275627489" />
      <inertia ixx="4.47942174809249E-05" ixy="2.06649562901671E-05" ixz="-6.5215728108858E-21" iyy="2.52108965590197E-05" iyz="-4.87207536330794E-21" izz="5.78162740600789E-05" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/prop3.STL" />
      </geometry>
      <material name="">
        <color rgba="0.752941176470588 0.752941176470588 0.752941176470588 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/prop3.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="f3" type="continuous">
    <origin xyz="-0.3 0 0.2005" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="prop3" />
    <axis xyz="0 0 -1" />
    <limit effort="0" velocity="0" />
  </joint>
  <link name="prop4">
    <inertial>
      <origin xyz="2.22044604925031E-16 1.38777878078145E-17 0.0210243882624621" rpy="0 0 0" />
      <mass value="0.0900308275627489" />
      <inertia ixx="2.52108965590196E-05" ixy="-2.0664956290167E-05" ixz="-9.979423408339E-21" iyy="4.47942174809251E-05" iyz="-2.16491563912817E-21" izz="5.78162740600789E-05" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/prop4.STL" />
      </geometry>
      <material name="">
        <color rgba="0.752941176470588 0.752941176470588 0.752941176470588 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="model://provant_alpha_quadrotor/meshes/prop4.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="f4" type="continuous">
    <origin xyz="0 -0.3 0.2005" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="prop4" />
    <axis xyz="0 0 -1" />
    <limit effort="0" velocity="0" />
  </joint>

  <!-- Defines the color of the links in gazebo -->
  <gazebo reference="base_link">
    <material>Gazebo/Red</material>
  </gazebo>

  <gazebo reference="legs">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="prop1">
    <material>Gazebo/White</material>
  </gazebo>

  <gazebo reference="prop2">
    <material>Gazebo/White</material>
  </gazebo>

  <gazebo reference="prop3">
    <material>Gazebo/White</material>
  </gazebo>

  <gazebo reference="prop4">
    <material>Gazebo/White</material>
  </gazebo>

  <!-- Loading the state and actuators plugins -->
  <gazebo>
    <plugin name="empuxo" filename="libgazebo_ros_QuadForces.so">
      <topic_F1>Empuxoh1</topic_F1>
      <topic_F2>Empuxoh2</topic_F2>
      <topic_F3>Empuxoh3</topic_F3>
      <topic_F4>Empuxoh4</topic_F4>
      <body>base_link</body>
      <Motor1>prop1</Motor1>
      <Motor2>prop2</Motor2>
      <Motor3>prop3</Motor3>
      <Motor4>prop4</Motor4>
      <DragCte>0.0179</DragCte>
    </plugin>
    <plugin name="vetor_estados" filename="libgazebo_ros_QuadData.so">
      <NameOfTopic>Estados</NameOfTopic>
      <bodyName>base_link</bodyName>
    </plugin>
  </gazebo>

</robot>