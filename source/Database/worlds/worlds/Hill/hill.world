    <include>
      <uri>model://cenario_hill</uri>
      <name>cenario_hill</name>
      <static>true</static>
      <pose>0 0 0 0 0 0</pose>
    </include>
    <scene>
      <sky>
        <time>18</time>
        <clouds>
          <speed>0</speed>
        </clouds>
      </sky>
    </scene>
